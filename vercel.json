{
  "version": 2,
  "builds": [
    {
      "src": "server-simple.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/css/$1",
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/img/(.*)",
      "dest": "/img/$1"
    },
    {
      "src": "/data/(.*)",
      "dest": "/data/$1",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/server-simple.js"
    },
    {
      "src": "/dashboard",
      "dest": "/server-simple.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server-simple.js"
    }
  ]
}
