{
  "version": 2,
  "builds": [
    {
      "src": "index-static.html",
      "use": "@vercel/static"
    },
    {
      "src": "css/**",
      "use": "@vercel/static"
    },
    {
      "src": "js/**",
      "use": "@vercel/static"
    },
    {
      "src": "img/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index-static.html"
    },
    {
      "src": "/index.html",
      "dest": "/index-static.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
